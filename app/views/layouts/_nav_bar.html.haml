      .logos
        %img.img-responsive.logoLetras{:alt => "enfoque-regional", :src => "../assets/logo_definitivo.png"}
    .col-md-7.col-sm-12
      .barra-menu
        %nav.navbar.navbar-default
          .container-fluid
            .navbar-header
              %button.navbar-toggle.collapsed{"data-target" => "#navcol-1", "data-toggle" => "collapse"}
